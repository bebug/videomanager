    	  }, 1500 );
    	});

    $("#buttondown").click(function(){
		contentmargin = Math.max($("#right").height() - $("#contentwrapper").height(), contentmargin - 200);
	  $("#contentwrapper").stop();
	  $("#contentwrapper").animate({
